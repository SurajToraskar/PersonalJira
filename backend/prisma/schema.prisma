generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model RefColumns {
  Id           Int     @id(map: "PK__RefColum__3214EC07BF3DC99F") @default(autoincrement())
  Name         String  @db.VarChar(200)
  Deleted      Boolean @default(false, map: "DF__RefColumn__Delet__49C3F6B7")
  DisplayOrder Int
}
